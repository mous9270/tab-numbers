{
  "manifest_version": 3,
  "name": "Tab Navigator",
  "version": "1.0",
  "description": "Displays a list of tabs when Alt is pressed and allows switching tabs.",
  "permissions": ["tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {}
}
